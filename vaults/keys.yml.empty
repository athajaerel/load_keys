# Rename this file to keys.yml and add your own keys.
# Don't try to use the default keys below --- for the purposes of
# this example, the encrypted strings are randomly generated. ;)
---
- keys:
  - name: 'preferably descriptive name - key 1'
    path: '~/.ssh/id_rsa_my_cool_thing_1'
    password: !vault |
              $ANSIBLE_VAULT;1.1;AES256
              
  - name: 'preferably descriptive name - key 2'
    path: '~/my_other_ssh_key_2'
    password: !vault |
              $ANSIBLE_VAULT;1.1;AES256
              
...
